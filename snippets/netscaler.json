{
	// Basic
	"Add Server": {
		"prefix": [
			"add server"
		],
		"body": "add server ${1:<name>} ${2:x.x.x.x}"
	},
	"Add Service": {
		"prefix": [
			"add service"
		],
		"body": "add service ${1:<name>} ${2:x.x.x.x} ${3:<serverName>} ${4|SSL,ADNS,ADNS_TCP,ANY,DHCPRA,DIAMETER,DNS,DNS_TCP,DTLS,FIX,FTP,GRE,HTTP,IPFIX,LOGSTREAM,MSSQL,MYSQL,NNTP,ORACLE,PPTP,QUIC,RADIUS,RADIUSListener,RDP,RPCSVR,RTSP,SIP_SSL,SIP_TCP,SIP_UDP,SMPP,SNMP,SSL_BRIDGE,SSL_DIAMETER,SSL_FIX,SSL_TCP,SYSLOGTCP,SYSLOGUDP,TCP,TFTP,UDP,USER_SSL_TCP,USER_TCP|} ${5:443}"
	},
	"Add Service Group": {
		"prefix": [
			"add servicegroup"
		],
		"body": "add servicegroup ${1:<name>} ${2|SSL,ADNS,ADNS_TCP,ANY,DHCPRA,DIAMETER,DNS,DNS_TCP,DTLS,FIX,FTP,GRE,HTTP,IPFIX,LOGSTREAM,MSSQL,MYSQL,NNTP,ORACLE,PPTP,QUIC,RADIUS,RADIUSListener,RDP,RPCSVR,RTSP,SIP_SSL,SIP_TCP,SIP_UDP,SMPP,SNMP,SSL_BRIDGE,SSL_DIAMETER,SSL_FIX,SSL_TCP,SYSLOGTCP,SYSLOGUDP,TCP,TFTP,UDP,USER_SSL_TCP,USER_TCP|} -appflowLog ${6|ENABLED,DISABLED|} -usip ${7|YES,NO|} -cip ${8|NO,YES X-Forwarded-For|}"
	},
	"Bind Service Group to Monitor": {
		"prefix": [
			"bind servicegroup MONITOR"
		],
		"body": "bind servicegroup ${1:<serviceGroupName>} -monitorName ${2:<monitorName>}"
	},
	"Bind Service Group to Server": {
		"prefix": [
			"bind servicegroup SERVER"
		],
		"body": "bind servicegroup ${1:<serviceGroupName>} ${2:<serverName>} ${3:443}"
	},
	// Content Switching
	"Add CS Action": {
		"prefix": [
			"add cs action",
		],
		"body": "add cs action ${1:<name>} -targetLBVserver ${2:<vserverName>}"
	},
	"Add CS Policy": {
		"prefix": [
			"add cs policy",
		],
		"body": "add cs policy ${1:<name>} -policy \"${2:policyExpression}\" -action ${3:actionName}"
	},
	"Add CS vServer": {
		"prefix": [
			"add cs vserver"
		],
		"body": "add cs vserver ${1:<name>} ${2|SSL,HTTP,TCP,FTP,RTSP,SSL_TCP,UDP,DNS,SIP_UDP,SIP_TCP,SIP_SSL,ANY,RADIUS,RDP,MYSQL,MSSQL,DIAMETER,SSL_DIAMETER,DNS_TCP,ORACLE,SMPP|} ${3:x.x.x.x} ${4:443} -cltTimeout ${5:180}"
	},
	"Bind CS vServer to Policy": {
		"prefix": [
			"bind cs vserver POLICY"
		],
		"body": "bind cs vserver ${1:<name>} -policyName ${2:<policyName>} -priority ${3:100} -type ${4|REQUEST,RESPONSE|}"
	},
	// Load Balancing
	"Add LB Monitor [HTTP-ECV]": {
		"prefix": [
			"add lb monitor HTTP-ECV",
			"http-ecv"
		],
		"body": "add lb monitor ${1:<namee>} HTTP-ECV -send \"${2:GET /}\" -recv \"${3:200 OK}\" -LRTM ${4|ENABLED,DISABLED|} -secure ${5|YES,NO|}"
	},
	"Add LB Monitor [TCP-ECV]": {
		"prefix": [
			"add lb monitor TCP-ECV",
			"tcp-ecv"
		],
		"body": "add lb monitor ${1:<name>} TCP-ECV -send \"${2:GET /}\" -recv \"${3:200 OK}\" -LRTM ${4|ENABLED,DISABLED|} -secure ${5|YES,NO|}"
	},
	"Add LB vServer": {
		"prefix": [
			"add lb vserver"
		],
		"body": "add lb vserver ${1:<name>} ${2|SSL,ADNS,ADNS_TCP,ANY,DHCPRA,DIAMETER,DNS,DNS_TCP,DTLS,FIX,FTP,GRE,HTTP,IPFIX,LOGSTREAM,MSSQL,MYSQL,NNTP,ORACLE,PPTP,QUIC,RADIUS,RADIUSListener,RDP,RPCSVR,RTSP,SIP_SSL,SIP_TCP,SIP_UDP,SMPP,SNMP,SSL_BRIDGE,SSL_DIAMETER,SSL_FIX,SSL_TCP,SYSLOGTCP,SYSLOGUDP,TCP,TFTP,UDP,USER_SSL_TCP,USER_TCP|} ${3:x.x.x.x} ${4:443} -cltTimeout ${5:180} -appflowLog ${6|ENABLED,DISABLED|}"
	},
	"Bind LB vServer to Policy": {
		"prefix": [
			"bind lb vserver POLICY"
		],
		"body": "bind lb vserver ${1:<name>} -policyName ${2:<policyName>} -priority ${3:100} -type ${4|REQUEST,RESPONSE|}"
	},
	"Bind LB vServer to Service Group": {
		"prefix": [
			"bind lb vserver SERVICE GROUP"
		],
		"body": "bind lb vserver ${1:<name>} ${2:<serviceGroupName>}"
	},
	// Networking
	"Add Subnet IP (SNIP)": {
		"prefix": [
			"add ns ip SNIP",
			"snip"
		],
		"body": "add ns ip ${1:x.x.x.x} ${2:255.255.255.0} -type SNIP -mgmtAccess DISABLED"
	},
	"Add Virtual IP (VIP))": {
		"prefix": [
			"add ns ip VIP",
			"vip"
		],
		"body": "add ns ip ${1:x.x.x.x} ${2:255.255.255.0} -vServer ENABLED"
	},
	// SSL
	"Bind SSL vServer to Certificate": {
		"prefix": [
			"bind ssl vserver CERTIFICATE"
		],
		"body": "bind ssl vserver ${1:<name>} -certkeyName ${2:<certkeyPairName>}"
	}
}

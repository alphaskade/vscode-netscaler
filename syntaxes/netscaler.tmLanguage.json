  "patterns": [{
      "match": "^[\\s]*#(.*)",
      "name": "comment.netscaler"
    },
    {
      "match": "\\s+[0-9]+\\s",
      "name": "constant.numeric.netscaler"
    },
    {
      "match": "(?<!\\w)\\d+\\.\\d+\\.\\d+\\.\\d+(/\\d{1,2})?(?!\\w)",
      "name": "constant.numeric.netscaler"
    },
    {
      "match": "[0-9a-fA-F]{0,4}:([0-9a-fA-F]{0,4}:)+[0-9a-fA-F]{0,4}(/\\d{1,3})?",
      "name": "constant.numeric.netscaler"
    },
    {
      "match": "\\b([0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}|\\h{1,4}\\.\\h{1,4}\\.\\h{1,4}(\\.\\h{1,2})?)\\b",

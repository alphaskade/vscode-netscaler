{
    "name": "netscaler",
    "displayName": "ns.conf",
    "description": "Syntax highlighter for Citrix ADC/NetScaler ns.conf files.",
    "version": "0.2.0",
    "publisher": "timdenholm",
    "license": "APACHE",
    "engines": {
        "vscode": "^1.5.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/timdenholm/vscode-netscaler/"
    },
    "bugs": {
        "url": "https://github.com/timdenholm/vscode-netscaler/issues/"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "netscaler",
            "aliases": [
                "NetScaler",
                "netscaler"
            ],
            "extensions": [
                ".conf",
                ".txt"
            ],
            "filenames": [
                "ns.conf"
            ],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "netscaler",
            "scopeName": "text.netscaler",
            "path": "./syntaxes/netscaler.tmLanguage.json"
        }]
    }
}
